	<DataTemplate
		DataType="{x:Type vm:HomeVM}"
		x:Key="{DataTemplateKey {x:Type vm:HomeVM}}">
		<view:Home />
	</DataTemplate>
	<DataTemplate
		DataType="{x:Type vm:CustomerVM}"
		x:Key="{DataTemplateKey {x:Type vm:CustomerVM}}">
		<view:Customers />
	</DataTemplate>
	<DataTemplate
		DataType="{x:Type vm:ProductVM}"
		x:Key="{DataTemplateKey {x:Type vm:ProductVM}}">
		<view:Products />
	</DataTemplate>
	<DataTemplate
		DataType="{x:Type vm:OrderVM}"
		x:Key="{DataTemplateKey {x:Type vm:OrderVM}}">
		<view:Orders />
	</DataTemplate>
	<DataTemplate
		DataType="{x:Type vm:SettingVM}"
		x:Key="{DataTemplateKey {x:Type vm:SettingVM}}">
		<view:Settings />
	</DataTemplate>
</ResourceDictionary>
