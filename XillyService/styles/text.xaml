<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style
		TargetType="{x:Type TextBlock}"
		x:Key="Text_Style">
		<Setter
			Property="FontFamily"
			Value="/Fonts/#Rubik" />
		<Setter
			Property="VerticalAlignment"
			Value="Center" />
		<Setter
			Property="HorizontalAlignment"
			Value="Left" />
		<Setter
			Property="Foreground"
			Value="#DBDBDB" />
		<Setter
			Property="FontSize"
			Value="18" />
		<Setter
			Property="Margin"
			Value="70,0,0,0" />
	</Style>
</ResourceDictionary>
